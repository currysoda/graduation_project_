//- register.js
html
    head
        title= `registerPage` 
    body
        h1 Register Account
          form(action='/account_register/register_confirm', method='POST')
            p
                | email:
                input(type='text', id=`email`, name='email', value='')
                br
                | password: 
                input(type='password',id=`password`, name='password', value='')
                br
                input(type='submit', value='Submit')